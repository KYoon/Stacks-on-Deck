          <h1 id="count">5</h1>
          <i id="down-arrow" class="fa fa-sort-desc fa-5x"></i>
        </div>
    </div>
        <h3 class="hand-size-label waiting-room">Starting Hand Size</h3>
        <div class="start-game waiting-room">
          <button class="btn btn-default" id="start-game-btn">Start Game</button>
        </div>
    </div>
  </div>





</div>