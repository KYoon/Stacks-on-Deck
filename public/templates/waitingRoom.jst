<!-- Waiting room -->
    <div class="container-fluid">
      <div class="room-key-div">
        <div><p class="navbar-brand">Room: <%= userData.roomkey %></p></div>
        <div class="navbar-inner">
          <div class="navbar-center inline">
            <p>Stacks on Deck</p>
          </div>
        </div>
      </div>
    </div>


 <div class="container" id="game-page-container">


<div class="row waiting-room">
   <div class="col-xs-12 welcome">
    <h2>Welcome <span id="username"><%= userData.username %></span></h2>
  </div>
</div>


  <div class="player-card-count row">
    <div class="col-xs-12">
      <div class="player-list col-xs-12">
        <h3>Players:</h3>
      </div>
    </div>
      </div>

    <div row>
    <div class="col-xs-12">
      <div class="inner-big" id="hand-count">
